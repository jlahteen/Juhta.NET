<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <DocumentationFile>Juhta.Net.Services.xml</DocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
